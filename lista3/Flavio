void retira_inicio_n(char *str, int n){
    int i, tamanho=strlen(str);
    if(n>tamanho){
        printf("\nerro\n");
        exit(1);
    }
    char *vetor = (char*)malloc(sizeof(char)*tamanho-n);
    if(!vetor)
        exit(1);
    for(i=n;i<tamanho;i++)
        vetor[i-n]=str[i];
    printf("\nEsta e a saida: %s\n",vetor);
}
